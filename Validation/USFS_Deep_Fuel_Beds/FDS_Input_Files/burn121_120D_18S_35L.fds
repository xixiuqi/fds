&HEAD CHID='burn121_120D_18S_35L', TITLE='USFS Deep Fuel Beds burn121 - 120 cm deep - 18 degree slope - 35 cm spacing' / 
!In EXP this run is clasified as a marginal burn with a burn percentage of 57.

! &MESH IJK=30,20,20, XB=-3,9,-3,3,0,6/ single coarse mesh for testing

! coarse outer mesh, dx=10 cm
&MULT ID='m1',DX=3,DY=2,DZ=2,
      I_LOWER=0,I_LOWER_SKIP=1,I_UPPER_SKIP=2,I_UPPER=3,
      J_LOWER=0,J_LOWER_SKIP=1,J_UPPER_SKIP=1,J_UPPER=2,
      K_LOWER=0,K_LOWER_SKIP=0,K_UPPER_SKIP=1,K_UPPER=2 / 36-4=32 meshes
&MESH IJK=30,20,20,XB=-3,0,-3,-1,0,2, MULT_ID='m1', COLOR='GRAY' /

! fine inner mesh, dx=2.5 cm
&MULT ID='m2',DX=1,DY=1,DZ=1,
      I_LOWER=0,I_UPPER=5,
      J_LOWER=0,J_UPPER=1,
      K_LOWER=0,K_UPPER=3 / 48 meshes
&MESH IJK=40,40,40,XB=0,1,-1,0,0,1, MULT_ID='m2', COLOR='BLACK' /

&TIME T_END=180, RESTRICT_TIME_STEP=F /
&DUMP DT_DEVC=0.5,DT_PART=0.5 /
&MISC SIMULATION_MODE='LES', HUMIDITY=67.8,TMPA=30.11 /

&REAC FUEL='FUEL VAPOR', C=2.10, H=6.20, O=2.16, SOOT_YIELD=0.01, HEAT_OF_COMBUSTION=17425. /

&SPEC ID='WATER VAPOR' /

&VENT DB='XMIN', SURF_ID='OPEN' /
&VENT DB='XMAX', SURF_ID='OPEN' /
&VENT DB='YMIN', SURF_ID='OPEN' /
&VENT DB='YMAX', SURF_ID='OPEN' /
&VENT DB='ZMAX', SURF_ID='OPEN' /

&SURF ID = 'wet vegetation'
      MATL_ID          = 'GENERIC VEGETATION'
      MOISTURE_CONTENT = 0.137
      SURFACE_VOLUME_RATIO = 7590
      LENGTH           = 0.1
      GEOMETRY         = 'CYLINDRICAL'
      COLOR            = 'SILVER' /

&CATF OTHER_FILES = 'vegetation_model.txt' /

&PART ID='EXCELSIOR', SAMPLING_FACTOR=1, SURF_ID='wet vegetation'
      QUANTITIES='PARTICLE TEMPERATURE','PARTICLE MASS','PARTICLE DIAMETER', STATIC=T 
      EMBER_PARTICLE=T, EMBER_DENSITY_THRESHOLD=300, EMBER_VELOCITY_THRESHOLD=0 /

- Vegetative fuel array
&MULT ID='FUEL_ARRAY', DXB=0.3382,0.3382,0,0,0.1099,0.1099, N_UPPER=12 /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.5334,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.3810,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.2286,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,0.0762,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.5334,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.3810,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.2286,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /
&INIT PART_ID='EXCELSIOR', XYZ=0.6762,-0.0762,0.101,
      N_PARTICLES=1000, MASS_PER_VOLUME=5.489, DRY=T, RADIUS=0.0762, HEIGHT=1.2192, SHAPE='CYLINDER',MULT_ID='FUEL_ARRAY' /

- Fine mesh Platform underneath fuel array and out 40 cm on each side
&SURF ID='platform',COLOR='GRAY'/
! &OBST XB=0.8,1.0,-1.45,1.45,0.0,0.2, SURF_ID='platform', MULT_ID='platform' /
! &MULT ID='platform',DXB=0.2,0.2,0,0,0.065,0.065, N_UPPER=21 /

&MOVE ID='mv1', X0=0.8, AXIS=0,1,0, ROTATION_ANGLE=-18 /
&GEOM XB=0.8,5.6,-1.45,1.45,-0.5,0, SURF_ID='platform', MOVE_ID='mv1'/

- Ignitor
&RAMP ID='RAMP IGN',T=0,  F=1.0 /
&RAMP ID='RAMP IGN',T=10, F=1.0 /
&RAMP ID='RAMP IGN',T=20, F=0.0 /
&RAMP ID='RAMP IGN',T=180, F=0.0 /

&SURF ID='ignitor', HRRPUA=500, COLOR='RED', RAMP_Q='RAMP IGN'/
&VENT XB=0.5,0.8,-0.80,0.80,0.0,0.0, SURF_ID='ignitor' /

&DEVC ID='Fuel Load', QUANTITY='MPUV', PART_ID='EXCELSIOR', SPATIAL_STATISTIC='VOLUME INTEGRAL', XB=-3,9,-4,4,0,6 /

&SLCF PBY=0.0, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=0.0, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBY=0.0, QUANTITY='HRRPUV', VECTOR=T /

&SLCF PBZ=0.5, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBZ=0.5, QUANTITY='TEMPERATURE', CELL_CENTERED=T /
&SLCF PBZ=0.5, QUANTITY='HRRPUV', VECTOR=T /

&TAIL /
